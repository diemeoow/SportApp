<Window x:Class="SportClub.Views.EditClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Редактировать клиента" Height="200" Width="400">
    <Grid Margin="10">
        <StackPanel>
            <Label Content="ФИО:"/>
            <TextBox Text="{Binding FullName}" Margin="0,5" />
            <Label Content="Номер:"/>
            <TextBox Text="{Binding Phone_num}" Margin="0,5" />
            <Button Content="Сохранить" Command="{Binding SaveCommand}" Margin="0,10" />
        </StackPanel>
    </Grid>
</Window>